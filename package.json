{
  "name": "fc-mde",
  "version": "1.0.0-alpha.46",
  "description": "React Markdown Editor (FC)",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "umd:main": "lib/umd/index.js",
  "types": "lib/types/index.d.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "webpack-dev-server --config ./config/webpack.config.demo.dev.js",
    "lang": "node ./tools/lang-builder.mjs",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node cleanup types && tsc -p config/tsconfig.typings.json",
    "clean": "node cleanup",
    "package": "npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmvan/fc-mde.git"
  },
  "keywords": [
    "react",
    "component",
    "markdown",
    "editor",
    "text-editor",
    "markdown-editor"
  ],
  "author": "Km.Van",
  "homepage": "https://github.com/kmvan/fc-mde.git#readme",
  "contributors": [
    {
      "name": "Andre Pena"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kmvan/fc-mde.git/issues"
  },
  "devDependencies": {
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/showdown": "^2.0.1",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "deep-sort-object": "^1.0.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.3.3",
    "html-webpack-plugin": "^5.5.3",
    "pofile": "^1.1.4",
    "prettier": "^3.0.0",
    "react-hot-loader": "^4.13.1",
    "react-is": "^18.2.0",
    "showdown": "^2.1.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "typescript-plugin-styled-components": "^3.0.0",
    "typescript-styled-plugin": "^0.18.2",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "fast-glob": "^3.3.0",
    "lucide-react": "^0.260.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.4"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "resolutions": {
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  }
}
