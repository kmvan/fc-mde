{
  "name": "fc-mde",
  "version": "1.0.0-alpha.36",
  "description": "React Markdown Editor (FC)",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "umd:main": "lib/umd/index.js",
  "types": "lib/types/index.d.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "webpack-dev-server --config ./config/webpack.config.demo.dev.js",
    "lang": "node ./build-lang.js",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node cleanup types && tsc -p config/tsconfig.typings.json",
    "clean": "node cleanup",
    "package": "npm run build && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmvan/react-mde.git"
  },
  "keywords": [
    "react",
    "component",
    "markdown",
    "editor",
    "text-editor",
    "markdown-editor"
  ],
  "author": "Km.Van",
  "homepage": "https://github.com/kmvan/react-mde.git#readme",
  "contributors": [
    {
      "name": "Andre Pena"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kmvan/react-mde.git/issues"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@types/showdown": "^1.9.4",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "deep-sort-object": "^1.0.2",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "glob": "^7.2.0",
    "html-webpack-plugin": "^5.5.0",
    "pofile": "^1.1.3",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hot-loader": "^4.13.0",
    "react-is": "^18.0.0",
    "sass": "^1.50.0",
    "showdown": "^2.0.3",
    "styled-components": "^5.3.5",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "typescript-plugin-styled-components": "^2.0.0",
    "typescript-styled-plugin": "^0.18.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "resolutions": {
    "@types/react": "18.0.0"
  }
}
