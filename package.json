{
  "name": "fc-mde",
  "version": "1.0.0-alpha.51",
  "description": "React Markdown Editor (FC)",
  "type": "module",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "umd:main": "lib/umd/index.js",
  "types": "lib/types/index.d.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./config/webpack.config.demo.dev.mjs",
    "lang": "node ./tools/lang-builder.mjs",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node cleanup.mjs cjs && rollup -c config/rollup.cjs.mjs",
    "build:esm": "node cleanup.mjs esm && rollup -c config/rollup.esm.mjs",
    "build:umd": "node cleanup.mjs umd && rollup -c config/rollup.umd.mjs",
    "build:types": "node cleanup.mjs types && tsc -p config/tsconfig.typings.json",
    "clean": "node cleanup.mjs",
    "package": "npm run build && npm pack"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmvan/fc-mde.git"
  },
  "keywords": [
    "react",
    "component",
    "markdown",
    "editor",
    "text-editor",
    "markdown-editor"
  ],
  "author": "Km.Van",
  "homepage": "https://github.com/kmvan/fc-mde.git#readme",
  "contributors": [
    {
      "name": "Andre Pena"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kmvan/fc-mde.git/issues"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/showdown": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "deep-sort-object": "^1.0.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.3.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "pofile": "^1.1.4",
    "prettier": "^3.0.0",
    "react-hot-loader": "^4.13.1",
    "react-is": "^18.2.0",
    "rollup": "^3.26.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "showdown": "^2.1.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "typescript-plugin-css-modules": "^5.0.1",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "lucide-react": "^0.260.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "showdown": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
